{
    "$schema": "https://json.schemastore.org/tsconfig",
    "_version": "22.0.0",
    "extends": "./base.json",
    "compilerOptions": {
        "lib": ["ES2022", "ESNext.Array"],
        "module": "ESNext",
        "target": "ES2022",
        "noEmit": false,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "bundler"
    }
}
